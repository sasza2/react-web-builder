{
  "name": "react-web-builder",
  "version": "0.21.0",
  "description": "React components for building pages",
  "main": "./dist/WebBuilder.cjs",
  "module": "./dist/WebBuilder.js",
  "type": "module",
  "types": "types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./types/index.d.ts"
      ],
      "components": [
        "./types/components.d.ts"
      ],
      "Box": [
        "./types/Box.d.ts"
      ],
      "useBoxStyle": [
        "./types/useBoxStyle.d.ts"
      ],
      "View": [
        "./types/View.d.ts"
      ]
    }
  },
  "engines": {
    "pnpm": "^10.0.0"
  },
  "files": [
    "dist/*",
    "types/*",
    "Box.js",
    "components.js",
    "View.js",
    "useBoxStyle.js"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/WebBuilder.js",
      "require": "./dist/WebBuilder.cjs"
    },
    "./components": {
      "types": "./types/components.d.ts",
      "import": "./dist/components.js",
      "require": "./dist/components.cjs"
    },
    "./Box": {
      "types": "./types/Box.d.ts",
      "import": "./dist/Box.js",
      "require": "./dist/Box.cjs"
    },
    "./useBoxStyle": {
      "types": "./types/useBoxStyle.d.ts",
      "import": "./dist/useBoxStyle.js",
      "require": "./dist/useBoxStyle.cjs"
    },
    "./View": {
      "types": "./types/View.d.ts",
      "import": "./dist/View.js",
      "require": "./dist/View.cjs"
    }
  },
  "keywords": [
    "react",
    "builder"
  ],
  "scripts": {
    "build:lib": "tsc && vite build --config vite.lib.config.ts",
    "build:demo": "storybook build",
    "changeset": "changeset",
    "dev": "storybook dev -p 6006",
    "e2e": "playwright test",
    "format": "biome format --write",
    "lint": "biome lint --write",
    "test": "vitest run --config vite.lib.config.ts src",
    "prepack": "pnpm build:lib",
    "publish:package": "changeset publish",
    "version:package": "changeset version",
    "storybook": "storybook dev -p 6006"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sasza2/react-web-builder.git"
  },
  "author": "sasza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sasza2/react-web-builder/issues"
  },
  "browser": {
    "[module-name]": false
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@biomejs/biome": "2.2.3",
    "@changesets/cli": "2.29.7",
    "@playwright/test": "1.55.0",
    "@storybook/react-vite": "9.1.5",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/babel__core": "7.20.5",
    "@types/react": "18.2.47",
    "@types/react-color": "3.0.13",
    "@types/react-dom": "18.2.7",
    "@types/react-redux": "7.1.34",
    "@types/react-toggle": "4.0.5",
    "@vitejs/plugin-react": "5.0.2",
    "@vitest/coverage-istanbul": "3.2.4",
    "babel-plugin-styled-components": "2.1.4",
    "changeset": "0.2.6",
    "html-dom-parser": "5.1.1",
    "jsdom": "26.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "9.1.5",
    "style-to-object": "1.0.9",
    "typescript": "5.9.2",
    "vite": "7.1.5",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.9.0",
    "@sasza/react-panzoom": "1.19.0",
    "arrows-svg": "1.8.1",
    "copy-to-clipboard": "3.3.3",
    "htmlparser2": "10.0.0",
    "i18next": "25.5.2",
    "postcss": "8.5.6",
    "postcss-selector-parser": "7.1.0",
    "postcss-value-parser": "4.2.0",
    "react-accessible-accordion": "5.0.1",
    "react-color": "2.19.3",
    "react-frame-component": "5.2.7",
    "react-grid-panzoom": "1.10.4",
    "react-i18next": "15.7.3",
    "react-player": "3.3.2",
    "react-range": "1.10.0",
    "react-redux": "9.2.0",
    "react-select": "5.10.2",
    "react-toastify": "11.0.5",
    "react-toggle": "4.1.3",
    "react-tooltip": "5.29.1",
    "slate": "0.118.1",
    "slate-react": "0.117.4",
    "styled-components": "6.1.19"
  }
}
