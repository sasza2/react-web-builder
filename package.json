{
  "name": "react-web-builder",
  "version": "0.1.0",
  "description": "React components for building pages",
  "main": "./dist/WebBuilder.cjs",
  "module": "./dist/WebBuilder.js",
  "type": "module",
  "types": "types/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./types/index.d.ts"
      ],
      "components": [
        "./types/components.d.ts"
      ],
      "Box": [
        "./types/Box.d.ts"
      ],
      "useBoxStyle": [
        "./types/useBoxStyle.d.ts"
      ],
      "View": [
        "./types/View.d.ts"
      ]
    }
  },
  "files": [
    "dist/*",
    "types/*",
    "Box.js",
    "components.js",
    "View.js",
    "useBoxStyle.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/WebBuilder.js",
      "require": "./dist/WebBuilder.cjs",
      "types": "./types/index.d.ts"
    },
    "./components": {
      "import": "./dist/components.js",
      "require": "./dist/components.cjs",
      "types": "./types/components.d.ts"
    },
    "./Box": {
      "import": "./dist/Box.js",
      "require": "./dist/Box.cjs",
      "types": "./types/Box.d.ts"
    },
    "./useBoxStyle": {
      "import": "./dist/useBoxStyle.js",
      "require": "./dist/useBoxStyle.cjs",
      "types": "./types/useBoxStyle.d.ts"
    },
    "./View": {
      "import": "./dist/View.js",
      "require": "./dist/View.cjs",
      "types": "./types/View.d.ts"
    }
  },
  "keywords": [
    "react",
    "pan",
    "zoom",
    "move",
    "panzoom",
    "moving",
    "selecting"
  ],
  "scripts": {
    "build:lib": "tsc && vite build --config vite.lib.config.ts",
    "build:demo": "ladle build",
    "dev": "ladle dev",
    "e2e": "playwright test",
    "lint": "eslint src tests --fix --cache",
    "test": "vitest run --coverage ./src --config vite.lib.config.ts",
    "prepack": "pnpm build:lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sasza2/react-panzoom.git"
  },
  "author": "sasza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sasza2/react-panzoom/issues"
  },
  "browser": {
    "[module-name]": false
  },
  "pnpm": {
    "overrides": {
      "csstype": "3.1.2"
    }
  },
  "devDependencies": {
    "@babel/core": "7.22.10",
    "@ladle/react": "4.0.2",
    "@playwright/test": "1.39.0",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/react": "14.0.0",
    "@types/babel__core": "7.20.1",
    "@types/react": "18.2.47",
    "@types/react-color": "3.0.6",
    "@types/react-dom": "18.2.7",
    "@types/react-redux": "7.1.25",
    "@types/react-toggle": "4.0.3",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vitejs/plugin-react": "4.0.4",
    "@vitest/coverage-istanbul": "0.34.6",
    "babel-plugin-styled-components": "2.1.4",
    "eslint": "8.50.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-dom-parser": "5.0.6",
    "jsdom": "22.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "style-to-object": "1.0.5",
    "typescript": "5.3.3",
    "vite": "4.3.5",
    "vitest": "0.34.6"
  },
  "peerDependencies": {
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "@sasza/react-panzoom": "1.12.0",
    "arrows-svg": "1.5.6",
    "copy-to-clipboard": "3.3.3",
    "i18next": "23.5.1",
    "react-accessible-accordion": "5.0.0",
    "react-color": "2.19.3",
    "react-frame-component": "5.2.6",
    "react-grid-panzoom": "1.3.0",
    "react-i18next": "13.2.2",
    "react-player": "2.13.0",
    "react-range": "1.8.14",
    "react-redux": "7.2.9",
    "react-select": "5.8.0",
    "react-toastify": "9.1.3",
    "react-toggle": "4.1.3",
    "react-tooltip": "5.18.0",
    "slate": "0.101.4",
    "slate-react": "0.101.3",
    "styled-components": "6.1.1"
  }
}
